<div class="jumbotron jumbotron-fluid">
    <h1 class="display-4"><a href="/">CarTech</a></h1>
    <p class="lead">Please user</p>
    <hr class="my-4">
</div>

<div id="home">
    <div class="container">
        <div class="row">
            <div class="col-8 offset-2">
                <form class="clearfix mb-4" action="POST">
                    <div class="form-group">
                        <h3>User:</h3>
                        <label for="cars-text">What is your Name</label>
                        <input type="text" id="user" class="form-control" aria-describedby="user-text" placeholder="Enter name here">
                        <button id="submit-user" class="btn btn-primary float-right">Submit</button>
                        <ul id="user-list" class="list-group">
                            {{#each customer}}
                            <li data-id="{{this.id}}" class="list-user-item">
                                <a href="user/{{this.id}}"></a>
                                <button class="btn btn-danger float-right delete">x</button>
                            </li>
                            {{/each}}
                        </ul>
                    </div>
            </div>
        </div>
    </div>
    <script type="text/javascript" src="https://cdnjs.cloudflare.com/ajax/libs/jquery/3.2.1/jquery.min.js"></script>
    <script src="/js/index.js"></script>